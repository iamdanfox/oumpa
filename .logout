#
# .logout - run at shell logout time
#
# $File: //systems/conf/LIVE/skel/logout $
# $Revision: #3 $
# $Date: 2002/12/17 $
# $Author: pod $
#
set runfrags=/etc/shellinit/tcsh/lib/runfrags
alias runfrags 'set _args=(\!*); if ( -f $runfrags ) source $runfrags'
set prefix=N
if ( $?prompt ) set prefix=I
runfrags $prefix /etc/shellinit/tcsh/logout.d
runfrags $prefix ~/.logout.d
